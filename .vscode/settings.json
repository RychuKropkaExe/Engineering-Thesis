        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.maxTokenizationLineLength": 2500
    },
    "advancedNewFile.convenienceOptions": [
        "last",
        "root"
    ],
    "advancedNewFile.exclude": {
        "node_modules": true,
        "build": true,
        "dist": true,
        "public": true
    },
    "auto-close-tag.disableOnLanguage": [
        "typescript"
    ],
    "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
    "breadcrumbs.enabled": false,
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.vcFormat.indent.preserveComments": true,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
    "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": true,
    "diffEditor.wordWrap": "on",
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.foldingImportsByDefault": true,
    "editor.fontFamily": "'Inconsolata NF','BitstreamVeraSansMono NF','SourceCodePro NF','Cascadia Code', 'Courier New', monospace",
    "editor.fontLigatures": false,
    "editor.fontSize": 15,
    "editor.fontWeight": 350,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 30,
    "editor.linkedEditing": true,
    "editor.minimap.renderCharacters": false,
    "editor.padding.top": 15,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.semanticTokenColorCustomizations": {
        "[One Monokai]": {
            "enabled": true,
            "rules": {
                "class": {
                    "foreground": "#d19a66",
                    "bold": true,
                    "italic": true
                },
                "namespace": {
                    "italic": true
                },
                "stringLiteral": {
                    "italic": true,
                    "bold": true
                },
                "number": {
                    "italic": true
                },
                "text": {
                    "italic": true
                },
                "enum": {
                    "italic": true
                },
                "constant": {
                    "italic": true
                },
                "comment": {
                    "italic": true
                },
                "enumMember": {
                    "foreground": "#9439af",
                    "italic": true
                },
                "function": {
                    "foreground": "#61afef",
                    "italic": true
                },
                "interface": {
                    "foreground": "#d19a66",
                    "bold": true,
                    "italic": true
                },
                "method": {
                    "foreground": "#61afef",
                    "italic": true,
                    "bold": true
                },
                "property": {
                    "foreground": "#43b6b9",
                    "italic": true
                },
                "type": {
                    "foreground": "#E5C07B",
                    "italic": true
                },
                "typeParameter": {
                    "foreground": "#E5C07B",
                    "bold": true,
                    "italic": true
                },
                "variable": {
                    "foreground": "#43b6b9",
                    "italic": true
                }
            }
        }
    },
    "editor.smoothScrolling": true,
    "editor.suggest.insertMode": "replace",
    "editor.suggest.showWords": false,
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "[One Monokai]": {
            "functions": "#61afef",
            "keywords": "#C678DD",
            "numbers": "#C678DD",
            "strings": "#98C379",
            "types": "#E5C07B",
            "variables": "#43b6b9",
            "comments": "#2fc9d4",
            "textMateRules": [
                {
                    "scope": [
                        "constant.language"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#D19A66"
                    }
                },
                {
                    "scope": [
                        "entity.name.function",
                        "support.function.js",
                        "support.type.property-name.css"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#61afef"
                    }
                },
                {
                    "scope": [
                        "punctuation.separator.key-value",
                        "storage.type.function.arrow.js"
                    ],
                    "settings": {
                        "foreground": "#E06C75"
                    }
                },
                {
                    "scope": [
                        "support.class.component"
                    ],
                    "settings": {
                        "foreground": "#E5C07B"
                    }
                },
                {
                    "scope": [
                        "punctuation.definition.typeparameters",
                        "keyword.operator.expression",
                        "storage.modifier",
                        "storage.type",
                        "storage.modifier.async",
                        "keyword.control",
                        "keyword.operator.new"
                    ],
                    "settings": {
                        "foreground": "#C678DD"
                    }
                },
                {
                    "scope": [
                        "entity.other.attribute-name"
                    ],
                    "settings": {
                        "foreground": "#d19a66",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "support.type.builtin",
                        "support.type.primitive"
                    ],
                    "settings": {
                        "foreground": "#E5C07B"
                    }
                },
                {
                    "scope": [
                        "variable.other.readwrite.alias",
                        "variable.other.property",
                        "variable.other.constant",
                        "variable.other.object"
                    ],
                    "settings": {
                        "foreground": "#43b6b9"
                    }
                }
            ]
        }
    },
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 1000,
    "eslint.format.enable": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
